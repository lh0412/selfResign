{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ],
          "chrome": "60",
          "firefox": "60",
          "safari": "10",
          "ie": "10",
          "edge": "17",
          "android": "4.0",
          "ios": "10"
        },
        "useBuiltIns": "usage"
        // "debug": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": {
          "version": 3,
          "proposals": true
        }
      }
    ]
  ]
}